{
    "functions": {
        "api/**/*.py": {
            "runtime": "@vercel/python@3.1.60"
        }
    },
    "routes": [
        {
            "src": "/img/sukasuka",
            "dest": "/api/img/sukasuka"
        },
        {
            "src": "/bing",
            "dest": "/api/bing"
        },
        {
            "src": "/bing/process",
            "dest": "/api/bing/process"
        }
    ]
}